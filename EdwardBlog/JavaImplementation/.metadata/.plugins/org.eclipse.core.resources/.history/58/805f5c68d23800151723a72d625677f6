  <script>
  $(function() {
	  var val=7;
    $( "#slider-range-max" ).slider({
      range: "max",
      min: 1,
      max: 15,
      value: val,
      slide: function( event, ui ) {
        $( "#amount" ).val( ui.value );
        val=ui.value;
        reset();
        drawCenteredTree(ui.value,15, 300);
      }
    });

    $( "#amount" ).val( $( "#slider-range-max" ).slider( "value" ) );
    drawCenteredTree(val, 15, 300);
  });
  function resetTree()
  {
	   drawCenteredTree(val, 15, 300);
  }
  </script>
